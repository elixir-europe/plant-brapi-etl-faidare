{
  "document-type": "study",
  "source-entity": "study",
  "document-transform": {
    "schema:url": "{.documentationURL}",
    "schema:includedInDataCatalog": "{.source}",
    "studyName": {
      "{or}": [
        "{.studyName}",
        "{.name}"
      ]
    },
    "location": {
        "locationName": {
          "{join}": "{.locationURI => .locationName}"
        }
    },
    "contacts": {
      "{list}": [{
        "email": {
          "{join}": "{.contactURIs => .email}"
        },
        "name": {
          "{join}": "{.contactURIs => .name}"
        },
        "orcid": {
          "{join}": "{.contactURIs => .orcid}"
        }
      }]
    },
    "seasons": {
      "{flatten_distinct}": {
        "{or}": [
          "{.seasons.season} {.seasons.year}",
          "{.seasons.season}",
          "{.seasons.year}",
          "{.seasons}"
        ]
      }
    }
  }
}