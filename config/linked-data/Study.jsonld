{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "brapi": "https://brapi.org/rdf/",
    "ppeo": "http://purl.org/ppeo/PPEO.owl#",

    "Study": "ppeo:study",
    "Trial": "ppeo:investigation",
    "Location": "ppeo:location",

    "studyPUI": "@id",
    "studyDbId": "ppeo:hasDatabaseId",
    "studyName": "ppeo:hasName",
    "name": "ppeo:hasName",
    "studyType": "brapi:hasType",
    "seasons": "ppeo:hasSeason",
    "studyDescription": "ppeo:hasDescription",
    "license": "ppeo:hasLicense",
    "startDate": "ppeo:hasStartDateTime",
    "endDate": "ppeo:hasEndDateTime",
    "active": "brapi:isActive",
    "trialDbIds": {
      "@id": "ppeo:investigation",
      "@type": "@id"
    },
    "locationPUI": {
      "@id": "brapi:hasLocation",
      "@type": "@id"
    },
    "germplasmPUI": {
      "@id": "brapi:hasGermplasm",
      "@type": "@id"
    },
    "germplasmDbIds": "brapi:hasGermplasm"
  }
}
