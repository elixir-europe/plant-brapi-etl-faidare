{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "brapi": "https://brapi.org/rdf/",
    "ppeo": "http://purl.org/ppeo/PPEO.owl#",

    "Germplasm": "ppeo:biological_material",
    "Donor": "brapi:Donor",

    "germplasmPUI": "@id",
    "germplasmDbId": "ppeo:hasDatabaseId",
    "accessionNumber": "ppeo:hasGermplasmAccessionNumber",
    "germplasmName": "ppeo:hasName",
    "genus": "ppeo:hasGenus",
    "species": "ppeo:hasSpecies",
    "speciesAuthority": "ppeo:hasSpeciesAuthority",
    "subtaxa": "ppeo:hasSubtaxon",
    "subtaxaAuthority": "ppeo:hasSubtaxonAuthority",
    "countryOfOriginCode": "ppeo:hasCountryOfOrigin",
    "instituteCode": "ppeo:hasInstituteCode",
    "instituteName": "ppeo:hasInstituteName",
    "biologicalStatusOfAccessionCode": "brapi:hasBiologicalStatusOfAccessionCode",
    "collectingOrAcquisitionSourceCode": "brapi:hasCollectingOrAcquisitionSourceCode",
    "typeOfGermplasmStorageCode": "brapi:hasTypeOfGermplasmStorageCode",
    "acquisitionDate": "brapi:hasAcquisitionDate",

    "donors": {
      "@id": "brapi:hasGermplasmDonor",
      "@type": "Donor"
    },
    "donorAccessionNumber": "brapi:hasAccessionNumber",
    "donorInstituteCode": "brapi:hasInstituteCode",
    "donorInstituteName": "brapi:hasInstituteName",
    "donorGermplasmPUI": {
      "@id": "brapi:isDerivedFromGermplasm",
      "@type": "@id"
    },
    "taxonIds": {
      "@id": "ppeo:hasTaxonIdentifier",
      "@type": "@id"
    }

  }
}
