{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "brapi": "https://brapi.org/rdf/",
    "ppeo": "http://purl.org/ppeo/PPEO.owl#",

    "Trial": "ppeo:trial",
    "Program": "brapi:Program",

    "trialPUI": "@id",
    "trialDbId": "ppeo:hasDatabaseId",
    "trialName": "ppeo:hasName",
    "startDate": "ppeo:hasStartDateTime",
    "endDate": "brapi:hasEndDateTime",
    "active": "brapi:hasActive",

    "programPUI": {
      "@id": "brapi:hasProgram",
      "@type": "@id"
    },

    "studyDbIds": "brapi:hasStudy",
    "studyPUI": "@id"
  }
}
